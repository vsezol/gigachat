{
  "root": "apps/client-terminal",
  "sourceRoot": "apps/client-terminal/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go build -o=../../dist/apps/client-terminal/ ./src/main.go",
        "cwd": "apps/client-terminal"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go run ./src/main.go",
        "cwd": "apps/client-terminal"
      }
    }
  }
}
